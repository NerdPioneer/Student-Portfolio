{
  "name": "student-portfolio",
  "version": "1.0.0",
  "description": "A modern student portfolio website built with Tailwind CSS and React",
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch:css & live-server --port=3000",
    "build": "npm run build:css",
    "watch:css": "tailwindcss -i ./src/input.css -o ./dist/styles.css --watch",
    "build:css": "tailwindcss -i ./src/input.css -o ./dist/styles.css --minify",
    "start": "live-server --port=3000"
  },
  "keywords": [
    "portfolio",
    "tailwindcss",
    "react",
    "student",
    "website"
  ],
  "author": "Student",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "live-server": "^1.2.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
